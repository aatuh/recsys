API_PORT=8000 # fill_in
DATABASE_URL=postgres://recsys:recsys@db:5432/recsys?sslmode=disable # fill_in
ENV=dev # fill_in dev, development, local (others are non-dev)
APP_DEBUG=true # fill_in
ORG_ID=00000000-0000-0000-0000-000000000001 # fill_in

POPULARITY_HALFLIFE_DAYS=14 # fill_in, integer
COVIS_WINDOW_DAYS=30 # fill_in
POPULARITY_FANOUT=1000 # fill_in
MMR_LAMBDA=0 # fill_in
BRAND_CAP=0 # fill_in
CATEGORY_CAP=0 # fill_in
BRAND_TAG_PREFIXES=brand # fill_in comma-separated prefixes
CATEGORY_TAG_PREFIXES=category,cat # fill_in comma-separated prefixes
RULE_EXCLUDE_EVENTS=false # fill_in
EXCLUDE_EVENT_TYPES=1,2,3 # fill_in, comma-separated event type ids
PURCHASED_WINDOW_DAYS=365 # fill_in
PROFILE_WINDOW_DAYS=14 # fill_in
PROFILE_BOOST=0.25 # fill_in, 0 disables personalization
PROFILE_TOP_N=32 # fill_in
BLEND_ALPHA=1.0 # fill_in
BLEND_BETA=0.0 # fill_in
BLEND_GAMMA=0.0 # fill_in
BANDIT_ALGO=thompson # fill_in, [thompson, ucb1]

MIGRATE_ON_START=true # fill_in [false, true]
MIGRATIONS_DIR=migrations # fill_in
SWAGGER_HOST=localhost:8000 # fill_in
SWAGGER_SCHEMES=https # fill_in http,https,comma,separated
CORS_ALLOWED_ORIGINS=127.0.0.1,localhost # fill_in comma,separated
CORS_ALLOW_CREDENTIALS=true # fill_in [false, true]
