{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "manifest.v1.schema.json",
  "title": "Artifact manifest v1",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "schema_version": {
      "const": "manifest.v1"
    },
    "tenant_id": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "version": {
      "type": "string"
    },
    "artifacts": {
      "type": "object"
    }
  },
  "required": [
    "schema_version",
    "tenant_id",
    "created_at",
    "version",
    "artifacts"
  ]
}
