{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . && pnpm -r --filter './packages/**' lint",
    "typecheck": "tsc --noEmit && pnpm -r --filter './packages/services/foo/**' typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate:api": "swagger2openapi -o /tmp/openapi.json ../api/swagger/swagger.json && openapi-typescript /tmp/openapi.json -o packages/services/foo/api-client/src/api/types.ts --path-params-types string"
  },
  "dependencies": {
    "@foo/config": "workspace:*",
    "@foo/api-client": "workspace:*",
    "@foo/domain": "workspace:*",
    "@foo/domain-adapters": "workspace:*",
    "@foo/hooks": "workspace:*",
    "@foo/i18n": "workspace:*",
    "@foo/legal": "workspace:*",
    "@api-boilerplate-core/content": "github:aatuh/api-boilerplate-core#path:/packages/content",
    "@api-boilerplate-core/env": "github:aatuh/api-boilerplate-core#path:/packages/env",
    "@api-boilerplate-core/http": "github:aatuh/api-boilerplate-core#path:/packages/http",
    "@api-boilerplate-core/i18n-shared": "github:aatuh/api-boilerplate-core#path:/packages/i18n-shared",
    "@api-boilerplate-core/layouts": "github:aatuh/api-boilerplate-core#path:/packages/layouts",
    "@api-boilerplate-core/legal": "github:aatuh/api-boilerplate-core#path:/packages/legal",
    "@api-boilerplate-core/theme": "github:aatuh/api-boilerplate-core#path:/packages/theme",
    "@api-boilerplate-core/ui": "github:aatuh/api-boilerplate-core#path:/packages/ui",
    "@api-boilerplate-core/widgets": "github:aatuh/api-boilerplate-core#path:/packages/widgets",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.3.0",
    "openapi-typescript": "^7.10.1",
    "swagger2openapi": "^7.0.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod": "^4.1.12"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
